{
    "name": "codelicia/immutability",
    "description": "Enforces immutability on initialized typed properties of any PHP object",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Jefersson Nathan",
            "email": "malukenho.dev@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Codelicia\\Immutable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodeliciaTest\\Immutable\\": "test/"
        }
    }
}
